<stack>
  <name>vision_opencv</name>
  <version>1.9.1</version>
  <description brief="vision_opencv">
    Packages for interfacing ROS with OpenCV, a library of programming
    functions for real time computer vision.
  </description>
  <author>Patrick Mihelich</author>
  <author>James Bowman</author>
  <maintainer email="vrabaud@willowgarage.com">Vincent Rabaud</maintainer>
  <copyright>Willow Garage</copyright>
  <license>BSD</license>  
  <review status="reviewed" notes=""/>
  <url>http://ros.org/wiki/vision_opencv</url>

  <build_depends>catkin</build_depends>
  <build_depends>common_msgs</build_depends> <!-- sensor_msgs -->
  <build_depends>geometry</build_depends> <!-- tf, tf_conversions -->
  <build_depends>opencv2</build_depends>
  <build_depends>ros</build_depends>
  <build_depends>ros_comm</build_depends> <!-- rostest, rospy, roscpp, message_filters -->

  <depends>common_msgs</depends>
  <depends>geometry</depends>
  <depends>opencv2</depends>
  <depends>ros</depends>
  <depends>ros_comm</depends>
</stack>
